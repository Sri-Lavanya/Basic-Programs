public class pallindrome1 {
	public static void main(String[] args) {
			int a=232;
			int  rem,reverse=0;
			int  b=a;
			while(a>0)
			{
				rem=a%10;
				reverse=(reverse*10)+rem;
				a=a/10;
			}
			if (b==reverse)
			{
			System.out.println("the given number is PALENDROME")	;
			}
			else
			{
				System.out.println(" the given number is NOT PALENDROME");
			}
		}
}
